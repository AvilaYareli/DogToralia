</a>\r\n            </div>\r\n            <div class=\"login-main\">\r\n              <form class=\"theme-form\" @submit.prevent=\"\">\r\n                <!-- Otros campos del formulario de login -->\r\n\r\n                <div v-if=\"errorMessage\" class=\"alert alert-danger\" role=\"alert\">\r\n                  <h4 class=\"alert-heading\" style=\"font-size: 16px;\">Advertencia!</h4>\r\n                  <p class=\"alert-heading\">{{ errorMessage }}</p>\r\n                </div>\r\n\r\n              </form>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n  <!-- Modal -->\r\n  <div class=\"modal fade\" id=\"scheduleModal\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"exampleModalLabel\" aria-hidden=\"true\">\r\n    <div class=\"modal-dialog\" role=\"document\">\r\n      <div class=\"modal-content\">\r\n        <div class=\"modal-header\">\r\n          <h5 class=\"modal-title\">¿Deseas guardar el mismo horario para los demás días?</h5>\r\n          <button class=\"btn-close\" type=\"button\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\r\n        </div>\r\n        <div class=\"modal-body\">\r\n          <p>Seleccionaste el lunes. ¿Quieres aplicar el mismo horario para otros días?</p>\r\n          <!-- Aquí puedes poner más campos si necesitas -->\r\n        </div>\r\n        <div class=\"modal-footer\">\r\n          <button class=\"btn btn-primary\" @click.prevent=\"applyScheduleToAllDays\" type=\"button\">Sí</button>\r\n          <button class=\"btn btn-secondary\" type=\"button\" data-bs-dismiss=\"modal\">No</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n  <!-- Selección de días -->\r\n  <div class=\"day-selection\">\r\n    <p>¿Deseas guardar el mismo horario para los siguientes días?</p>\r\n    <div>\r\n      <label v-for=\"(day, index) in days\" :key=\"index\">\r\n        <input type=\"checkbox\" :value=\"day\" v-model=\"selectedDays\" @change=\"checkMondaySelected(day)\">\r\n        {{ day }}\r\n      </label>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\n   import axios from 'axios';\r\n   import { mapActions } from 'vuex'; \r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      type: 'password',\r\n      email: '',\r\n      password: '',\r\n      errorMessage: '',\r\n    };\r\n    \r\n  },\r\n  methods: {\r\n    showPassword(){\r\n      this.type = this.type === 'password' ? 'text' : 'password';\r\n    },\r\n    menu(){\r\n        this.$router.replace('/');\r\n      },\r\n      ...mapActions(['login']),\r\n    async doLogin() {\r\n      try {\r\n        const userType = await this.login({ email: this.email, password: this.password });\r\n        if (userType === 'pet_owner') {\r\n          this.$router.push('/petOwner');\r\n          this.$store.dispatch('menu/initializeMenu');\r\n        } else if (userType === 'business_owner') {\r\n          // this.$router.push('/busOwner');\r\n          this.$router.push('/listar-Minegocio');\r\n          this.$store.dispatch('menu/initializeMenu');\r\n        }else if (userType === 'staff') {\r\n          this.$router.push('/busOwner');\r\n          this.$store.dispatch('menu/initializeMenu');\r\n        }\r\n      } catch (error) {\r\n        console.error('Error en el login:', error);\r\n        this.errorMessage = error.message || 'Error desconocido al iniciar sesión.';\r\n      }\r\n    },\r\n    \r\n    \r\n    //\r\n\r\n    ...mapActions(['updateUserData']),\r\n    updateMenuBasedOnUser() {\r\n      const userType = localStorage.getItem('userType') || 'user'; // o admin\r\n      this.updateUserData(userType);\r\n   \r\n  },\r\n  },\r\n  mounted() {\r\n    this.updateMenuBasedOnUser();\r\n  }\r\n};\r\n</script>\r\n<style>\r\n.logo img {\r\n  width: 300px;\r\n  height: auto;\r\n}\r\n</style>"],"mappings":";;;;;;AAgEG,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,UAAS,QAAS,MAAM;AAEpC,eAAe;EACbC,IAAI,WAAAA,KAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,UAAU;MAChBC,KAAK,EA