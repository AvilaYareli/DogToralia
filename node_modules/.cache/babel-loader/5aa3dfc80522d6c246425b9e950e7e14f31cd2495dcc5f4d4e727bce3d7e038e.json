{"ast":null,"code":"import userProfile from \"@/components/users/userProfile.vue\";\nimport userUpdate from \"@/components/users/userUpdate.vue\";\nexport default {\n  components: {\n    userProfile: userProfile,\n    userUpdate: userUpdate\n  },\n  data: function data() {\n    return {\n      token: localStorage.getItem('token'),\n      userType: localStorage.getItem('userType') // 'pet_owner' o 'business_owner'\n    };\n  },\n  computed: {\n    isPetOwner: function isPetOwner() {\n      return this.userType === 'pet_owner';\n    },\n    isBusinessOwner: function isBusinessOwner() {\n      return this.userType === 'business_owner';\n    }\n  }\n  //    mounted() {\n  //   this.token = localStorage.getItem('token');\n  //   this.userType = localStorage.getItem('userType');\n  // },\n};","map":{"version":3,"names":["userProfile","userUpdate","components","data","token","localStorage","getItem","userType","computed","isPetOwner","isBusinessOwner"],"sources":["C:\\YARELI\\New\\VUE\\DogToralia\\src\\pages\\user\\userEdit.vue"],"sourcesContent":["<!-- \r\n<template>\r\n  <div class=\"col-xl-8 col-lg-7\">--\r\n      <form class=\"card\" @submit.prevent=\"updatedatos\">\r\n      <div class=\"card-header pb-0\">\r\n        <h3 class=\"mb-1 f-20 txt-primary\">Mi información personal</h3>\r\n        <div class=\"card-options\">\r\n          <a class=\"card-options-collapse\" href=\"#\" data-bs-toggle=\"card-collapse\"><i class=\"fe fe-chevron-up\"></i></a>\r\n          <a class=\"card-options-remove\" href=\"#\" data-bs-toggle=\"card-remove\"><i class=\"fe fe-x\"></i></a>\r\n        </div>\r\n      </div>\r\n      <div class=\"card-body\">\r\n        <div class=\"row align-items-center\"> \r\n    \r\n                <div class=\"col-sm-3 col-md-3 d-flex flex-column align-items-center\">\r\n                  <div class=\"avatar mb-3\">\r\n                    <img class=\"img-fluid\" :src=\"getFotoUrl(datauser.profile_photo )\"alt=\"Foto de perfil\">\r\n                  </div>\r\n                  <input ref=\"fileInput\" type=\"file\" @change=\"onFileChange\" class=\"d-none\">\r\n                  <button @click.prevent=\"triggerFileInput\" class=\"btn btn-light\">Subir Imagen</button>\r\n                </div>\r\n          <div class=\"col-sm-9 col-md-9\">\r\n            <div class=\"row\">\r\n              <div class=\"col-sm-6 col-md-4\">\r\n                <div class=\"mb-3\">\r\n                  <label class=\"form-label f-w-500\">Nombre</label>\r\n                  <input v-model=\"datauser.name\" class=\"form-control\" @input=\"filterName\">\r\n                </div>\r\n              </div>\r\n              <div class=\"col-sm-6 col-md-4\">\r\n                <div class=\"mb-3\">\r\n                  <label class=\"form-label f-w-500\">Apellido</label>\r\n                  <input v-model=\"datauser.last_name\" class=\"form-control\" @input=\"filterApe\">\r\n                </div>\r\n              </div>\r\n              <div class=\"col-sm-6 col-md-4\">\r\n                <div class=\"mb-3\">\r\n                  <label class=\"form-label f-w-500\">Correo</label>\r\n                  <input v-model=\"datauser.email\" class=\"form-control\" @input=\"filterApe\" readonly>\r\n                </div>\r\n              </div>\r\n              <div class=\"col-sm-6 col-md-4\">\r\n                <div class=\"mb-3\">\r\n                  <label class=\"form-label f-w-500\">Número de teléfono</label>\r\n                  <input v-model=\"datauser.phone_number\" class=\"form-control\" @input=\"filterNumbers\" maxlength=\"10\">\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"card-footer d-flex justify-content-end\">\r\n        <button class=\"btn btn-primary\" type=\"submit\" @click=\"editarperfil\">Actualizar cambios</button>\r\n      </div>\r\n      \r\n    </form>\r\n\r\n    <div v-if=\"errorMessage\" class=\"alert alert-danger\" role=\"alert\">\r\n      <h4 class=\"alert-heading\" style=\"font-size: 16px;\">Advertencia!</h4>\r\n      <p class=\"alert-heading\">{{ errorMessage }}</p>\r\n    </div>\r\n\r\n    <div v-if=\"okMessage\" class=\"alert alert-success\" role=\"alert\">\r\n      <h4 class=\"alert-heading\" style=\"font-size: 16px;\">Éxito!</h4>\r\n      <p class=\"alert-heading\">Has actualizado tus datos.</p>\r\n    </div>\r\n \r\n</template>\r\n<script>\r\nexport default {\r\n    data() {\r\n      return {\r\n        datauser:{},\r\n        // editPet:{},\r\n        perId: this.$route.params.id,\r\n        errorMessage: '',\r\n        okMessage: '',\r\n      };\r\n    },\r\n\r\n    watch: {\r\n      '$route.params.id': function(newId) {\r\n        this.perId = newId;\r\n        editarMimascota(); // Llama a verDetalles cuando cambia el ID\r\n        mostrarInfo();\r\n      }\r\n    },\r\n    \r\n    created() {\r\n      this.mostrarInfo();\r\n    },\r\n    methods: {\r\n      triggerFileInput() {\r\n    this.$refs.fileInput.click();\r\n  },\r\n  onFileChange(event) {\r\n    const file = event.target.files[0];\r\n    if (file) {\r\n      const reader = new FileReader();\r\n      reader.onload = e => {\r\n        this.profilePhotoPreview = e.target.result;\r\n      };\r\n      reader.readAsDataURL(file);\r\n    }\r\n  },\r\n      getFotoUrl(profile_photo) {\r\n      const baseUrl = 'http://127.0.0.1:8000/storage/';\r\n      return `${baseUrl}${profile_photo}`;\r\n     },\r\n     \r\n    async mostrarInfo(){\r\n      try {\r\n          \r\n          const token=localStorage.getItem('token');\r\n          const myHeaders = new Headers();\r\n          myHeaders.append(\"Content-Type\", \"application/json\");\r\n          myHeaders.append(\"Authorization\", `Bearer ${token}`);\r\n\r\n          console.log('ID de la mascota:', this.perId);\r\n\r\n            const requestOptions = {\r\n            method: \"GET\",\r\n            headers: myHeaders,\r\n            redirect: \"follow\"\r\n            };\r\n\r\n            // fetch(`https://devs-devitm.com/api/pets/${this.perId}`, requestOptions)\r\n            const response = await fetch(`http://127.0.0.1:8000/api/pet_owner/me`, requestOptions)\r\n            const result = await response.json();           \r\n                    this.datauser= datos.user;\r\n                    console.log('dato:',this.datauser);   \r\n            \r\n        } catch (err) {\r\n          console.error('¿Cuál es el error?', err);\r\n  \r\n          if (err.response && err.response.data && err.response.data.message) {\r\n            this.error = err.response.data.message;\r\n          } else {\r\n            this.error = 'Error al obtener los detalles del dueño de mascotas';\r\n          }\r\n  \r\n          this.datauser = null;\r\n        }\r\n    },\r\n     \r\n    async editarperfil(){\r\n        try {\r\n          const token=localStorage.getItem('token');\r\n          const myHeaders = new Headers();\r\n          myHeaders.append(\"Content-Type\", \"application/json\");\r\n          myHeaders.append(\"Authorization\", `Bearer ${token}`);\r\n\r\n          console.log('ID de la mascota:', this.perId);\r\n\r\n          const raw = JSON.stringify({\r\n            \"name\": this.datauser.name,\r\n            \"last_name\":this.datauser.last_name,\r\n            \"phone_number\": this.datauser.phone_number,\r\n          //   \"profile_photo\":this.datauser.profile_photo ,\r\n           \r\n            });  \r\n            console.log('que paso aqui?',raw);\r\n\r\n\r\n        const requestOptions = {\r\n        method: \"PUT\",\r\n        headers: myHeaders,\r\n        body: raw,\r\n        redirect: \"follow\"\r\n        };\r\n\r\n            // fetch(`https://devs-devitm.com/api/pets/${this.perId}`, requestOptions)\r\n            const response = await fetch(`http://127.0.0.1:8000/api/pet_owner/update`, requestOptions);\r\n            const result = await response.json();\r\n\r\n              if (result.status === \"error\") {\r\n              // this.errorMessage = result.message;\r\n              this.errorMessage = `${result.message}: ${result.error}`;\r\n              this.okMessage = '';\r\n            }else{\r\n              this.okMessage = 'Datos actualizados correctamente';\r\n              this.errorMessage = '';\r\n              this.datauser = {\r\n                name: '',\r\n                last_name: '',\r\n                phone_number: '',\r\n              //   profile_photo: '',\r\n              };\r\n              await this.mostrarInfo();\r\n            }\r\n               \r\n        } catch (err) {\r\n        console.error('¿Cuál es el error?', err);\r\n\r\n        if (err.response && err.response.data && err.response.data.message) {\r\n            this.error = err.response.data.message;\r\n        } else {\r\n            this.error = 'Error al obtener los detalles del dueño de mascotas';\r\n        }\r\n\r\n        this.datauser = null;\r\n    }\r\n      },\r\n\r\n }\r\n\r\n\r\n}\r\n\r\n</script>\r\n<style>\r\n.avatar {\r\nwidth: 150px; /* Ajusta el tamaño según sea necesario */\r\nheight: 150px; /* Asegúrate de que el ancho y la altura sean iguales para un círculo perfecto */\r\noverflow: hidden; /* Asegura que la imagen no se salga del contenedor */\r\nborder-radius: 50%; /* Hace que el contenedor sea circular */\r\ndisplay: flex;\r\nalign-items: center;\r\njustify-content: center;\r\n}\r\n\r\n.avatar img {\r\nwidth: 100%;\r\nheight: auto;\r\nobject-fit: cover; /* Asegura que la imagen cubra el contenedor sin distorsión */\r\n}\r\n\r\n\r\n</style>\r\n{{{{{{{{{{{{{{{{{{{{{{{{{{{{{{{}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}} -->\r\n\r\n<template>\r\n    <Breadcrumbs main=\"User\" title=\"Ajustes\"/>\r\n    <div class=\"container-fluid\">\r\n           <div class=\"edit-profile\">\r\n             <div class=\"row\">\r\n                 <userProfile/> \r\n                 <userUpdate/>\r\n\r\n             </div>\r\n           </div>\r\n    </div>\r\n</template>\r\n<script>\r\n\r\nimport userProfile from \"@/components/users/userProfile.vue\"\r\nimport userUpdate from \"@/components/users/userUpdate.vue\"\r\nexport default {\r\n   \r\n    components:{\r\n       userProfile,\r\n       userUpdate,\r\n   },\r\n   data() {\r\n      \r\n      return {\r\n        token: localStorage.getItem('token'),\r\n        userType: localStorage.getItem('userType') // 'pet_owner' o 'business_owner'\r\n      };\r\n    },\r\n    computed: {\r\n      isPetOwner() {\r\n        return this.userType === 'pet_owner';\r\n      },\r\n      isBusinessOwner() {\r\n        return this.userType === 'business_owner';\r\n      }\r\n    },\r\n//    mounted() {\r\n//   this.token = localStorage.getItem('token');\r\n//   this.userType = localStorage.getItem('userType');\r\n// },\r\n}\r\n\r\n</script> "],"mappings":"AAqPA,OAAOA,WAAU,MAAO,oCAAmC;AAC3D,OAAOC,UAAS,MAAO,mCAAkC;AACzD,eAAe;EAEXC,UAAU,EAAC;IACRF,WAAW,EAAXA,WAAW;IACXC,UAAU,EAAVA;EACJ,CAAC;EACDE,IAAI,WAAAA,KAAA,EAAG;IAEJ,OAAO;MACLC,KAAK,EAAEC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;MACpCC,QAAQ,EAAEF,YAAY,CAACC,OAAO,CAAC,UAAU,EAAE;IAC7C,CAAC;EACH,CAAC;EACDE,QAAQ,EAAE;IACRC,UAAU,WAAAA,WAAA,EAAG;MACX,OAAO,IAAI,CAACF,QAAO,KAAM,WAAW;IACtC,CAAC;IACDG,eAAe,WAAAA,gBAAA,EAAG;MAChB,OAAO,IAAI,CAACH,QAAO,KAAM,gBAAgB;IAC3C;EACF;EACJ;EACA;EACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}