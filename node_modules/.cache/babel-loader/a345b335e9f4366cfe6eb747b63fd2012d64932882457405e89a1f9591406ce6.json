e-toastification';\r\n    \r\n    export default {\r\n      data() {\r\n        return {\r\n          dataserv: {},\r\n          servId: this.$route.params.id,\r\n          errorMessage: '',\r\n          okMessage: '',\r\n        };\r\n      },\r\n    \r\n      watch: {\r\n        '$route.params.id': function(newId) {\r\n          this.servId = newId;\r\n          this.mostrarInfo();\r\n        }\r\n      },\r\n      \r\n      created() {\r\n        this.mostrarInfo();\r\n      },\r\n      methods: {\r\n        getFotoUrl(profile_photo) {\r\n          const baseUrl = 'http://127.0.0.1:8000/storage/';\r\n          return `${baseUrl}${profile_photo}`;\r\n        },\r\n        async mostrarInfo(){\r\n          try {\r\n            const token = localStorage.getItem('token');\r\n            const myHeaders = new Headers();\r\n            myHeaders.append(\"Content-Type\", \"application/json\");\r\n            myHeaders.append(\"Authorization\", `Bearer ${token}`);\r\n    \r\n            const requestOptions = {\r\n              method: \"GET\",\r\n              headers: myHeaders,\r\n              redirect: \"follow\"\r\n            };\r\n    \r\n            const response = await fetch(`http://127.0.0.1:8000/api/staff/${this.servId}`, requestOptions);\r\n            const result = await response.text();\r\n            const datos = JSON.parse(result);\r\n            this.dataserv = datos.staff;\r\n          } catch (err) {\r\n            this.errorMessage = 'Error al obtener los detalles del miembro del personal';\r\n            console.error(err);\r\n          }\r\n        },\r\n        async editarperfil(){\r\n          const toast = useToast();\r\n          try {\r\n            const token = localStorage.getItem('token');\r\n            const myHeaders = new Headers();\r\n            myHeaders.append(\"Content-Type\", \"application/json\");\r\n            myHeaders.append(\"Authorization\", `Bearer ${token}`);\r\n    \r\n            const raw = JSON.stringify({\r\n 