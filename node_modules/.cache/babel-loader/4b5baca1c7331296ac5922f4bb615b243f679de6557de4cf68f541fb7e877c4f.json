<input v-model=\"editPet.species\" class=\"form-control\" @input=\"filterApe\">\r\n            </div>\r\n          </div>\r\n          <div class=\"col-sm-6 col-md-6\">\r\n            <div class=\"mb-3\">\r\n              <label class=\"form-label f-w-500\">Raza</label>\r\n              <input v-model=\"editPet.breed\" class=\"form-control\" @input=\"filterApe\">\r\n            </div>\r\n          </div>\r\n          <!-- calendarioooooooooooo -->\r\n\r\n          <div class=\"col-sm-6 col-md-6\">\r\n              <div class=\"mb-3\">\r\n              <label  class=\"form-label f-w-500\">Fecha de nacimiento</label>\r\n              <input class=\"datepicker-here form-control\" v-model=\"editPet.birth_date\"  type=\"date\" data-language=\"en\">\r\n              </div>\r\n          </div> \r\n          <!--  -->\r\n          <div class=\"col-sm-6 col-md-6\">\r\n            <div class=\"mb-3\">\r\n              <label class=\"form-label f-w-500\">Color</label>\r\n              <input v-model=\"editPet.color\" class=\"form-control\" @input=\"filterApe\">\r\n            </div>\r\n          </div>\r\n          <div class=\"col-sm-6 col-md-6\">\r\n            <div class=\"mb-3\">\r\n              <label class=\"form-label f-w-500\">Género</label>\r\n              <input v-model=\"editPet.gender\" class=\"form-control\"  @input=\"filterNumbers\"  maxlength=\"10\">\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"card-footer d-flex justify-content-between\">\r\n        <router-link class=\"btn btn-success active\" to=\"/mis-mascotas\">Regresar</router-link>\r\n        <button class=\"btn btn-primary\" type=\"submit\" @click=\"editarMimascota\">Actualizar Datos</button>\r\n      </div>\r\n    </form>\r\n\r\n    <div v-if=\"errorMessage\" class=\"alert alert-danger\" role=\"alert\">\r\n      <h4 class=\"alert-heading\" style=\"font-size: 16px;\">Advertencia!</h4>\r\n      <p class=\"alert-heading\">{{ errorMessage }}</p>\r\n    