n\r\n            fetch(`https://devs-devitm.com/api/pets/${this.petId}`, requestOptions)\r\n            .then((response) => response.text())\r\n            .then((result) =>{\r\n                    let datos = JSON.parse(result);             \r\n                    // this.editPet = datos;\r\n                    // console.log('dato:',this.editPet );  \r\n                    this.okMessage = 'Datos actualizados correctamente';\r\n                    this.errorMessage = '';\r\n                    this.nombre= '';\r\n                    this.especie= '';\r\n                    this.raza= '';\r\n                    this.fecha = '';\r\n                    this.color = '';\r\n                    this.genero= '';\r\n            }\r\n             )\r\n            \r\n        } catch (err) {\r\n          console.error('¿Cuál es el error?', err);\r\n  \r\n          if (err.response && err.response.data && err.response.data.message) {\r\n            this.error = err.response.data.message;\r\n          } else {\r\n            this.error = 'Error al obtener los detalles del dueño de mascotas';\r\n          }\r\n  \r\n          this.datapet = null;\r\n        }\r\n      },\r\n\r\n }\r\n\r\n\r\n}\r\n\r\n</script>"],"mappings":";;;;;;;;;AA0EA,eAAe;EACXA,IAAI,WAAAA,KAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAC,CAAC,CAAC;MACVC,KAAK,EAAE,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAE;MAC5BC,YAAY,EAAE,EAAE;MAChBC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EAEDC,KAAK,EAAE;IACL,kBAAkB,EAAE,SAAAC,eAASC,KAAK,EAAE;MAClC,IAAI,CAACR,KAAI,GAAIQ,KAAK;MAClBC,eAAe,CAAC,CAAC,EAAE;IAErB;EACF,CAAC;EACDC,OAAO,EAAE;IACLC,cAAc,WAAAA,eAACC,KAAK,EAAE;MACxB,IAAAC,YAAA,GAA2BD,KAAK,CAACE,KAAK,CAAC,GAAG,CAAC;QAAAC,aAAA,GAAAC,cAAA,CAAAH,YAAA;QAApCI,IAAI,GAAAF,aAAA;QAAEG,KAAK,GAAAH,aAAA;QAAEI,GAAG,GAAAJ,aAAA;MACvB,IAAMK,CAAC,MAAAC,MAAA,CAAIF,GAAG,OAAAE,MAAA,CAAIH,KAAK,OAAAG,MAAA,CAAIJ,IAAI,CAAC;MAChCK,OAAO,CAACC,GAAG,CAAC,UAAU,EAACH,CAAC;MACxB,OAAOA,CAAC;IACV,CAAC;IACKX,eAAe,WAAAA,gBAAA,EAAE;MAAA,IAAAe,KAAA;MAAA,OAAAC,iBAAA,eAAAC,mBAAA,GAAA