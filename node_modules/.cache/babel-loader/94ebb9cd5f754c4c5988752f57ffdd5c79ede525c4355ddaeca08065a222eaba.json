\n        reader.onload = e => {\r\n          this.PhotoPreview = e.target.result;\r\n        };\r\n        reader.readAsDataURL(file);\r\n      }\r\n    },\r\n    async Regismascota() {\r\n\r\n        const token = localStorage.getItem('token');\r\n        if (!token) {\r\n          this.errorMessage = 'No se encontró ningún token, redireccionando para iniciar sesión';\r\n          this.$router.push('/auth/login');\r\n          return;\r\n        }\r\n\r\n      try { \r\n        const error = this.validarCampos({\r\n            nombre: this.nombre,\r\n            especie: this.especie,\r\n            raza:this.raza ,\r\n            fecha:this.fecha ,\r\n            color: this.color,\r\n            genero:this.genero ,\r\n           \r\n          });\r\n          if (error) {\r\n            this.errorMessage = error;\r\n            toast.warning(error);\r\n            return;\r\n          }\r\n          \r\n          const token=localStorage.getItem('token');\r\n          const formData = new FormData();\r\n          formData.append('name', this.nombre);\r\n          formData.append('species', this.especie);\r\n          formData.append('breed', this.raza);\r\n          formData.append('birth_date', this.formatearFecha(this.fecha));\r\n          formData.append('color', this.color);\r\n          formData.append('gender', this.genero);\r\n          if (this.$refs.fileInput.files.length > 0) {\r\n          formData.append('image', this.$refs.fileInput.files[0]);\r\n           } \r\n\r\n\r\n      // fetch(\"https://devs-devitm.com/api/pets\", requestOptions)\r\n      const response= await fetch(\"http://127.0.0.1:8000/api/pets\", {\r\n        method: 'POST',\r\n      headers: {\r\n        'Authorization': `Bearer ${token}`\r\n      },\r\n      body: formData\r\n      });\r\n      const result = await response.json();\r\n      console.log('ppppppppp',result.status);\r\n        if( result.status ==\"success\"){\r\n          console.log('interesante',result);\r\n        