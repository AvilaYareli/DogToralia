 h-100 d-flex flex-column\">\r\n      <div class=\"card flex-grow-1\">\r\n                <!--         -->\r\n                <div class=\"card-header\">\r\n                <!-- <a class=\"fa fa-paw fa-lg\" @click=\"agregar(pet.id)\">Agregar mascota</a> -->\r\n                <a class=\"fa fa-paw fa-lg icon\" @click=\"agregar\">Agregar mascota</a>\r\n                </div>\r\n                <!--  -->\r\n                \r\n        <div class=\"card-body\">\r\n          <h1 class=\"text-center\">Mis mascotas registradas</h1>\r\n          <div class=\"d-flex flex-column align-items-start mb-4\">\r\n            <div class=\"w-100\">\r\n              <table class=\"table table-striped\">\r\n                <thead class=\"table-dark\">\r\n                  <tr>\r\n                    <th>Id</th>\r\n                    <th>Nombre</th>\r\n                    <th>Especie</th>\r\n                    <th>Raza</th>\r\n                    <th >Acciones</th>\r\n                    \r\n                    <!-- <th>Id dueño</th> -->\r\n                  </tr>\r\n                </thead>\r\n                <tbody>\r\n                  <tr v-for=\"pet in pet\" :key=\"pet.id\">\r\n                    <td>{{pet.id }}</td>\r\n                    <td>{{pet.name}}</td>\r\n                    <td>{{pet.species }}</td>\r\n                    <td>{{pet.breed  }}</td>\r\n                    <td class=\"text-center\">\r\n                        <button class=\"btn btn-outline-info mx-1\"  @click=\"detalles(pet.id)\">\r\n                            <i class=\"fa fa-eye  fa-lg\"></i>\r\n                        </button>\r\n\r\n                        <button class=\"btn btn-outline-danger mx-1\"  @click=\"eliminar(pet.id)\">\r\n                                <i class=\"fa fa-trash-o  fa-lg\"></i>\r\n                        </button>\r\n\r\n                        <button class=\"btn btn-outline-success-2x \"  @click=\"editar(pet.id)\">\r\n                                <i class=\"fa fa-penci