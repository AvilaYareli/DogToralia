\n            'Authorization': `Bearer ${token}`,\r\n           \r\n             },\r\n          }\r\n          \r\n        );\r\n        console.log('QUE OCURRE?',formData);\r\n        if (response.data.status === 'error') {\r\n          this.errorMessage = response.data.message;\r\n        } else {\r\n          this.okMessage = 'Datos actualizados correctamente';\r\n          this.errorMessage = '';\r\n          this.nombre = '';\r\n          this.apellido = '';\r\n          this.telefono = '';\r\n          this.foto = null;\r\n          this.profilePhotoPreview = '';\r\n        }\r\n      } catch (error) {\r\n        console.error('Error al actualizar el perfil', error);\r\n        this.errorMessage = 'Hubo un problema al actualizar el perfil';\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.avatar img {\r\n  width: 100px;\r\n  height: 100px;\r\n\r\n}\r\n\r\n.row.align-items-center {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n</style>"],"mappings":";;;;;AA+DA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,WAAAA,KAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE,IAAI;MACVC,mBAAmB,EAAE,EAAE;MACvBC,YAAY,EAAE,EAAE;MAChBC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAU,WAAAA,WAACC,KAAK,EAAE;MAChB,IAAMC,oBAAmB,GAAID,KAAK,CAACE,MAAM,CAACC,KAAK,CAACC,OAAO,CAAC,YAAY,EAAE,EAAE,CAAC;MACzE,IAAI,CAACb,MAAK,GAAIU,oBAAoB;IACpC,CAAC;IACDI,SAAS,WAAAA,UAACL,KAAK,EAAE;MACf,IAAMC,oBAAmB,GAAID,KAAK,CAACE,MAAM,CAACC,KAAK,CAACC,OAAO,CAAC,YAAY,EAAE,EAAE,CAAC;MACzE,IAAI,CAACZ,QAAO,GAAIS,oBAAoB;IACtC,CAAC;IACDK,aAAa,WAAAA,cAACN,KAAK,EAAE;MACnB,IAAI,CAACJ,YAAW,GAAI,EAAE;MACtB,IAAMW,WAAU,GAAIP,KAAK,CAACE,MAAM,CAACC,KAAK,CAACC,OAAO,CAAC,KAAK,EAAE,EAAE,CAAC;MACzD,IAAI,CAACX,QAAO,GAAIc,WAAW;IAC7B,CAAC;IACDC,gBAAgB,WAAAA,iBAAA,EAAG;MACjB,IAAI,CAACC,KAAK,CAACC,SAAS,CAACC,KAAK,CAAC,CAAC;IAC9B,CAAC;IACDC,YAAY,WAAAA,aAACZ,KAAK,EAAE;MAAA,IAAAa,KAAA;MAClB,IAAMC,IAAG,GAAId,KAAK,CAACE,MAAM,CAACa,KAAK,CAAC,CAAC,CAAC;MAClC